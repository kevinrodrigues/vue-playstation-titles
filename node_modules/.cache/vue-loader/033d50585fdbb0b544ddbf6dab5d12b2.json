rn this.$store.state.game.games;\n        },\n    },\n    methods: {\n        ...mapGetters('game', ['reviewedGames']),\n    },\n}\n</script>\n\n<style scoped lang=\"scss\">\n.ratings {\n    a {\n        padding: 10px;\n        display: block;\n    }\n    \n    li {\n        border-top: 1px solid #fff;\n        margin-top: 5px;\n        background: rgba(0,0,0,.6);\n\n        &:first-child {\n            border-top: none;\n            margin-top: 0;\n        }\n    }\n}\n\n.review-container {\n    width: 100%;\n    padding: 5px;\n    overflow: hidden;\n    \n    img {\n        width: 80px;\n        height: auto;\n        float: left;\n    }\n}\n    \n.description {\n    float: left;\n    margin-left: 25px;\n    max-width: 500px;\n}\n\n.star-rating:not(old){\n    display        : inline-block;\n    width          : 7.5em;\n    height         : 1.5em;\n    overflow       : hidden;\n    vertical-align : bottom;\n}\n\n.star-rating:not(old) > input{\n    margin-right : -100%;\n    opacity      : 0;\n}\n\n.star-rating:not(old) > label{\n    display         : block;\n    float           : right;\n    position        : relative;\n    background      : url('../../img/star-off.svg');\n    background-size : contain;\n}\n\n.star-rating:not(old) > label:before{\n    content         : '';\n    display         : block;\n    width           : 1.5em;\n    height          : 1.5em;\n    background      : url('../../img/star-on.svg');\n    background-size : contain;\n    opacity         : 0;\n    transition      : opacity 0.2s linear;\n}\n\n.star-rating:not(old) > label:hover:before,\n.star-rating:not(old) > label:hover ~ label:before,\n.star-rating:not(:hover) > :checked ~ label:before{\n    opacity : 1;\n}\n</style>\n"]}]}